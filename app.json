{
  "expo": {
    "name": "ColorHunt",
    "slug": "ColorHunt-recipes-app",
    "description": "React Native Recipes App Demo.",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#007AFF"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.colorhuntmobileapp.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSNotificationsUsageDescription": "We need your permission to send you notifications"
      }
    },
    "android": {
      "package": "com.colorhuntmobileapp.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "firebase": {
        "projectId": "b0d5d035-7a66-4a0f-b5ec-33b84d030443"
      },
      "eas": {
        "projectId": "b0d5d035-7a66-4a0f-b5ec-33b84d030443"
      }
    }
  }
}
